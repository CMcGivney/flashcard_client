    </div>\n          <Card.Description>{question}</Card.Description>\n          <hr></hr>\n          <Button.Group vertical fluid>\n            <Button onClick={toggleOne}>\n              Hint #1 {hintOne ? \": '\" + hint1 + \"'\" : \"\"}\n            </Button>\n            <Button onClick={toggleTwo}>\n              Hint #2 {hintTwo ? \": '\" + hint2 + \"'\" : \"\"}\n            </Button>\n            <Button onClick={toggleThree}>\n              Hint #3 {hintThree ? \": '\" + hint3 + \"'\" : \"\"}\n            </Button>\n            <Button onClick={toggleAnswer}>\n              Answer {answerOn ? \": '\" + answer + \"'\" : \"\"}\n            </Button>\n          </Button.Group>\n        </Card.Content>\n        <Card.Content extra>\n          <div>\n            <LikeButton user={user} flash={{ id, likes, likeCount }} />\n            {user && user.username === username && (\n              <DeleteButton flashId={id} callback={deleteFlashCallback} />\n            )}\n          </div>\n        </Card.Content>\n      </Card>\n    );\n  }\n  return postMarkup;\n}\n\nconst FETCH_ONE_FLASH_QUERY = gql`\n  query ($flashId: ID!) {\n    getFlash(flashId: $flashId) {\n      id\n      category\n      question\n      hint1\n      hint2\n      hint3\n      answer\n      createdAt\n      username\n      likeCount\n      likes {\n        username\n        createdAt\n      }\n    }\n  }\n`;\n\nexport default SingleFlash;\n","/Users/chrismcgivney/Desktop/hack-portfolio/client/src/pages/Login.js",[],["156","157"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/FlashForm.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/DeleteButton.js",[],["158","159"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/pages/Home.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/NavBar.js",[],["160","161"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/util/hooks.js",[],["162","163"],"/Users/chrismcgivney/Desktop/hack-