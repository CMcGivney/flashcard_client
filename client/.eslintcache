c-ui-react\";\nimport { Link } from \"react-router-dom\";\nimport moment from \"moment\";\n\nimport { AuthContext } from \"../context/auth\";\nimport LikeButton from \"../components/LikeButton.js\";\nimport DeleteButton from \"../components/DeleteButton.js\";\nimport useToggle from \"../util/toggle.js\";\n\nfunction SingleFlash(props) {\n  const flashId = props.match.params.flashId;\n  const { user } = useContext(AuthContext);\n\n  const [hintOne, toggleOne] = useToggle();\n  const [hintTwo, toggleTwo] = useToggle();\n  const [hintThree, toggleThree] = useToggle();\n  const [answerOn, toggleAnswer] = useToggle();\n\n  const { data } = useQuery(FETCH_ONE_FLASH_QUERY, {\n    variables: {\n      flashId,\n    },\n  });\n\n  function deleteFlashCallback() {\n    props.history.push(\"/home\");\n  }\n\n  let postMarkup;\n\n  if (!data) {\n    postMarkup = <p>Loading flashcard...</p>;\n  } else {\n    const {\n      id,\n      category,\n      question,\n      hint1,\n      hint2,\n      hint3,\n      answer,\n      createdAt,\n      username,\n      likes,\n      likeCount,\n    } = data.getFlash;\n\n    postMarkup = (\n      <Card fluid>\n        <Card.Content>\n          <Card.Header>{category}</Card.Header>\n          <div\n            style={{\n              display: \"flex\",\n              flexFlow: \"row nowrap\",\n              justifyContent: \"space-around\",\n            }}\n          >\n            <Card.Meta>createdBy: {username}</Card.Meta>\n            <Card.Meta>{moment(createdAt).fromNow(true)} ago</Card.Meta>\n          </div>\n          <Card.Description>{question}</Card.Description>\n          <hr></hr>\n          <Button.Group vertical fluid>\n            <Button onClick={toggleOne}>\n              Hint #1 {hintOne ? \": '\" + hint1 + \"'\" : \"\"}\n            </Button>\n            <Button onClick={toggleTwo}>\n              Hint #2 {hintTwo ? \": '\" + hint2 + \"'\" : \"\"}\n            </Button>\n            <Button onClick={toggleThree}>\n              Hint #3 {hintThree ? \": '\" + hint3 + \"'\" : \"\"}\n            </Button>\n            <Button onClick={toggleAnswer}>\n              Answer {answerOn ? \": '\" + answer + \"'\" : \"\"}\n            </Button>\n          </Button.Group>\n        </Card.Content>\n        <Card.Content extra>\n          <div>\n            <LikeButton user={user} flash={{ id, likes, likeCount }} />\n            {user && user.username === username && (\n              <DeleteButton flashId={id} callback={deleteFlashCallback} />\n            )}\n          </div>\n        </Card.Content>\n      </Card>\n    );\n  }\n  return postMarkup;\n}\n\nconst FETCH_ONE_FLASH_QUERY = gql`\n  query ($flashId: ID!) {\n    getFlash(flashId: $flashId) {\n      id\n      category\n      question\n      hint1\n      hint2\n      hint3\n      answer\n      createdAt\n      username\n      likeCount\n      likes {\n        username\n        createdAt\n      }\n    }\n  }\n`;\n\nexport default SingleFlash;\n","/Users/chrismcgivney/Desktop/hack-portfolio/client/src/pages/Login.js",[],["151","152"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/FlashForm.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/DeleteButton.js",[],["153","154"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/pages/Home.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/NavBar.js",[],["155","156"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/util/hooks.js",[],["157","158"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/FlashCard.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/ApolloProvider.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/index.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/context/auth.js",[],["159","160"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/pages/Landing.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/util/toggle.js",[],"/Users/