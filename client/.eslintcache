ikes {\n        username\n        createdAt\n      }\n    }\n  }\n`;\n\nexport default SingleFlash;\n","/Users/chrismcgivney/Desktop/hack-portfolio/client/src/pages/Login.js",[],["138","139"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/FlashForm.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/DeleteButton.js",[],["140","141"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/pages/Home.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/NavBar.js",[],["142","143"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/util/hooks.js",[],["144","145"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/FlashCard.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/ApolloProvider.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/index.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/context/auth.js",[],["146","147"],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/pages/Landing.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/util/toggle.js",[],"/Users/chrismcgivney/Desktop/hack-portfolio/client/src/components/Categories.js",[],["148","149"],"/Users/chrismcgivney/Desktop/flashcard-client/client/src/ApolloProvider.js",[],"/Users/chrismcgivney/Desktop/flashcard-client/client/src/components/NavBar.js",[],["150","151"],"/Users/chrismcgivney/Desktop/flashcard-client/client/src/pages/Landing.js",[],"/Users/chrismcgivney/Desktop/flashcard-client/client/src/util/toggle.js",[],"/Users/chrismcgivney/Desktop/flashcard-client/client/src/util/hooks.js",[],"/Users/chrismcgivney/Desktop/flashcard-client/client/src/index.js",[],"/Users/chrismcgivney/Desktop/flashcard-client/client/src/App.js",[],"/Users/chrismcgivney/Desktop/flashcard-client/client/src/pages/Home.js",[],"/Users/chrismcgivney/Desktop/flashcard-client/client/src/pages/SingleFlash.js",["152"],"/Users/chrismcgivney/Desktop/flashcard-client/client/src/components/FlashForm.js